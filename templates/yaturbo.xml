		{% for item in turbopages %}
        <item turbo="true">
            <turbo:extendedHtml>true</turbo:extendedHtml>
            <link>https://{{ request.get_host }}{{ item.get_absolute_url }}</link>
            <author>DOMOPROEKTOR</author>
            <category>Советы</category>
            <pubDate>{{ item.publish_date|date:'c' }}</pubDate>
            <turbo:content>
                <![CDATA[
                    <header>
					<h1>{{ item.title }}</h1>
					<p>{{ item.shortdescription }}</p>
					<figure>
					<img src="https://{{ request.get_host }}/{{ item.image }}">
					</figure>					
                    </header>
					{{ item.description|safe }}
				]]>
			</turbo:content>			
		</item>
		{% endfor %}
		
